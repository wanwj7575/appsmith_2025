  "srcDoc": "\n<!DOCTYPE html>\n<html lang=\"zh-CN\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>财务统计看板</title>\n    <script src=\"https://cdn.tailwindcss.com\"></script>\n    <script src=\"https://cdn.jsdelivr.net/npm/chart.js\"></script>\n    <link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css\">\n    <style>\n        :root {\n            --primary-color: #3b82f6;\n            --secondary-color: #10b981;\n            --danger-color: #ef4444;\n            --warning-color: #f59e0b;\n            --info-color: #6b7280;\n        }\n        \n        .stat-card {\n            transition: all 0.3s ease;\n            border-left: 4px solid var(--primary-color);\n        }\n        \n        .stat-card:hover {\n            transform: translateY(-2px);\n            box-shadow: 0 10px 15px -3px rgba(0, 0, 0, 0.1);\n        }\n        \n        .progress-bar {\n            transition: width 0.5s ease-in-out;\n        }\n        \n        .fade-in {\n            animation: fadeIn 0.5s ease-in;\n        }\n        \n        @keyframes fadeIn {\n            from { opacity: 0; transform: translateY(10px); }\n            to { opacity: 1; transform: translateY(0); }\n        }\n        \n        .chart-container {\n            background: white;\n            border-radius: 12px;\n            padding: 20px;\n            box-shadow: 0 4px 6px -1px rgba(0, 0, 0, 0.1);\n        }\n    </style>\n</head>\n<body class=\"bg-gray-50 min-h-screen\">\n    <div class=\"container mx-auto px-4 py-6\">\n        <!-- 标题区域 -->\n        <div class=\"flex items-center justify-between mb-8\">\n            <div class=\"flex items-center space-x-3\">\n                <div class=\"w-10 h-10 bg-blue-600 rounded-lg flex items-center justify-center\">\n                <i class=\"fas fa-chart-line text-white\"></i>\n            </div>\n            <h1 class=\"text-2xl font-bold text-gray-800\">财务统计看板</h1>\n        </div>\n        <div class=\"flex items-center space-x-4\">\n            <select id=\"periodSelect\" class=\"bg-white border border-gray-300 rounded-lg px-4 py-2 text-gray-700 focus:outline-none focus:ring-2 focus:ring-blue-500\">\n                <option value=\"monthly\">月度</option>\n                <option value=\"quarterly\">季度</option>\n                <option value=\"yearly\">年度</option>\n            </select>\n            <button id=\"refreshBtn\" class=\"bg-blue-600 hover:bg-blue-700 text-white px-4 py-2 rounded-lg transition-colors\">\n                <i class=\"fas fa-sync-alt mr-2\"></i>刷新\n            </button>\n        </div>\n    </div>\n\n    <!-- KPI指标卡片 -->\n    <div class=\"grid grid-cols-1 md:grid-cols-2 lg:grid-cols-4 gap-4 mb-8\">\n        <div class=\"stat-card bg-white rounded-lg p-6\">\n            <div class=\"flex items-center justify-between\">\n                <div>\n                    <p class=\"text-gray-500 text-sm\">总收入</p>\n                    <h3 class=\"text-2xl font-bold text-gray-800 mt-1\">¥ 2,845,600</h3>\n                    <div class=\"flex items-center mt-2\">\n                        <span class=\"text-green-500 text-sm\">\n                            <i class=\"fas fa-arrow-up mr-1\"></i>12.5%\n                        </span>\n                    </div>\n                </div>\n                <div class=\"w-12 h-12 bg-green-100 rounded-full flex items-center justify-center\">\n                    <i class=\"fas fa-dollar-sign text-green-600 text-xl\"></i>\n                </div>\n            </div>\n        </div>\n        \n        <div class=\"stat-card bg-white rounded-lg p-6\">\n            <div class=\"flex items-center justify-between\">\n                <div>\n                    <p class=\"text-gray-500 text-sm\">净利润</p>\n                    <h3 class=\"text-2xl font-bold text-gray-800 mt-1\">¥ 587,200</h3>\n                    <div class=\"flex items-center mt-2\">\n                        <span class=\"text-green-500 text-sm\">\n                            <i class=\"fas fa-arrow-up mr-1\"></i>8.3%\n                        </span>\n                    </div>\n                </div>\n                <div class=\"w-12 h-12 bg-blue-100 rounded-full flex items-center justify-center\">\n                <i class=\"fas fa-chart-pie text-blue-600 text-xl\"></i>\n            </div>\n        </div>\n    </div>\n    \n    <div class=\"stat-card bg-white rounded-lg p-6\">\n        <div class=\"flex items-center justify-between\">\n            <div>\n                <p class=\"text-gray-500 text-sm\">运营成本</p>\n                <h3 class=\"text-2xl font-bold text-gray-800 mt-1\">¥ 1,284,300</h3>\n                <div class=\"flex items-center mt-2\">\n                    <span class=\"text-red-500 text-sm\">\n                        <i class=\"fas fa-arrow-down mr-1\"></i>5.2%\n                </span>\n            </div>\n        </div>\n        <div class=\"w-12 h-12 bg-red-100 rounded-full flex items-center justify-center\">\n                <i class=\"fas fa-money-bill-wave text-red-600 text-xl\"></i>\n            </div>\n        </div>\n    </div>\n    \n    <div class=\"stat-card bg-white rounded-lg p-6\">\n            <div class=\"flex items-center justify-between\">\n                <div>\n                    <p class=\"text-gray-500 text-sm\">现金流</p>\n                <h3 class=\"text-2xl font-bold text-gray-800 mt-1\">¥ 892,400</h3>\n                <div class=\"flex items-center mt-2\">\n                    <span class=\"text-green-500 text-sm\">\n                <i class=\"fas fa-arrow-up mr-1\"></i>15.7%\n            </span>\n        </div>\n    </div>\n    <div class=\"w-12 h-12 bg-purple-100 rounded-full flex items-center justify-center\">\n            <i class=\"fas fa-exchange-alt text-purple-600 text-xl\"></i>\n        </div>\n    </div>\n</div>\n\n<!-- 图表区域 -->\n<div class=\"grid grid-cols-1 lg:grid-cols-2 gap-6 mb-8\">\n    <!-- 收入趋势图 -->\n    <div class=\"chart-container\">\n        <h3 class=\"text-lg font-semibold text-gray-800 mb-4\">月度收入趋势</h3>\n    <div class=\"h-64\">\n        <canvas id=\"revenueChart\"></canvas>\n    </div>\n</div>\n\n<!-- 费用分布图 -->\n<div class=\"chart-container\">\n    <h3 class=\"text-lg font-semibold text-gray-800 mb-4\">费用分布</h3>\n    <div class=\"h-64\">\n        <canvas id=\"expenseChart\"></canvas>\n    </div>\n</div>\n\n<!-- 预算执行情况 -->\n<div class=\"chart-container lg:col-span-2\">\n    <h3 class=\"text-lg font-semibold text-gray-800 mb-4\">预算执行情况</h3>\n    <div class=\"h-64\">\n        <canvas id=\"budgetChart\"></canvas>\n    </div>\n</div>\n\n<!-- 财务指标表格 -->\n<div class=\"chart-container lg:col-span-2\">\n    <h3 class=\"text-lg font-semibold text-gray-800 mb-4\">财务指标详情</h3>\n    <div class=\"overflow-x-auto\">\n        <table class=\"min-w-full bg-white\">\n            <thead>\n                <tr class=\"bg-gray-50\">\n                    <th class=\"px-4 py-3 text-left text-xs font-medium text-gray-500 uppercase\">指标名称</th>\n                    <th class=\"px-4 py-3 text-left text-xs font-medium text-gray-500 uppercase\">当前值</th>\n                    <th class=\"px-4 py-3 text-left text-xs font-medium text-gray-500 uppercase\">目标值</th>\n                    <th class=\"px-4 py-3 text-left text-xs font-medium text-gray-500 uppercase\">完成率</th>\n                    <th class=\"px-4 py-3 text-left text-xs font-medium text-gray-500 uppercase\">状态</th>\n                </tr>\n            </thead>\n            <tbody class=\"divide-y divide-gray-200\">\n                <tr>\n                    <td class=\"px-4 py-3 text-sm text-gray-900\">收入增长率</td>\n                    <td class=\"px-4 py-3 text-sm text-gray-500\">12.5%</td>\n                    <td class=\"px-4 py-3 text-sm text-gray-500\">10.0%</td>\n                    <td class=\"px-4 py-3 text-sm text-gray-500\">125%</td>\n                    <td class=\"px-4 py-3\">\n                        <span class=\"px-2 py-1 text-xs bg-green-100 text-green-800 rounded-full\">超额完成</span>\n                    </td>\n                </tr>\n                <tr>\n                    <td class=\"px-4 py-3 text-sm text-gray-900\">净利润率</td>\n                    <td class=\"px-4 py-3 text-sm text-gray-500\">20.6%</td>\n                    <td class=\"px-4 py-3 text-sm text-gray-500\">18.0%</td>\n                    <td class=\"px-4 py-3 text-sm text-gray-500\">114%</td>\n                    <td class=\"px-4 py-3\">\n                        <span class=\"px-2 py-1 text-xs bg-green-100 text-green-800 rounded-full\">良好</span>\n                    </td>\n                </tr>\n                <tr>\n                    <td class=\"px-4 py-3 text-sm text-gray-900\">成本控制率</td>\n                    <td class=\"px-4 py-3 text-sm text-gray-500\">95.2%</td>\n                    <td class=\"px-4 py-3 text-sm text-gray-500\">93.0%</td>\n                    <td class=\"px-4 py-3 text-sm text-gray-500\">102%</td>\n                    <td class=\"px-4 py-3\">\n                        <span class=\"px-2 py-1 text-xs bg-blue-100 text-blue-800 rounded-full\">达标</span>\n                    </td>\n                </tr>\n            </tbody>\n        </table>\n    </div>\n</div>\n</div>\n\n<script>\n    // 财务数据管理\n    class FinancialDashboard {\n        constructor() {\n            this.currentPeriod = 'monthly';\n            this.data = this.loadData();\n            this.init();\n        }\n\n        init() {\n            this.setupEventListeners();\n            this.initCharts();\n        }\n\n        loadData() {\n            return {\n                monthly: {\n                    revenue: [185, 210, 198, 245, 280, 310, 295, 320, 335, 352, 368, 390],\n                    expenses: [135, 150, 145, 170, 190, 210, 200, 215, 225, 235, 245, 255],\n                    profit: [50, 60, 53, 75, 90, 100, 95, 105, 110, 117, 123, 135]\n                },\n                quarterly: {\n                    revenue: [605, 835, 925, 1110],\n                    expenses: [430, 570, 640, 735],\n                    profit: [175, 265, 285, 375]\n                },\n                yearly: {\n                    revenue: [3450],\n                    expenses: [2470],\n                    profit: [980]\n                }\n            };\n        }\n\n        setupEventListeners() {\n            document.getElementById('periodSelect').addEventListener('change', (e) => {\n                this.currentPeriod = e.target.value;\n                this.updateCharts();\n            });\n\n            document.getElementById('refreshBtn').addEventListener('click', () => {\n                this.refreshData();\n            });\n        }\n\n        initCharts() {\n            // 收入趋势图\n            this.revenueChart = new Chart(\n                document.getElementById('revenueChart').getContext('2d'), \n                {\n                    type: 'line',\n                    data: {\n                        labels: ['1月', '2月', '3月', '4月', '5月', '6月', '7月', '8月', '9月', '10月', '11月', '12月'],\n                        datasets: [{\n                            label: '收入',\n                            data: this.data.monthly.revenue,\n                            borderColor: '#3B82F6',\n                            backgroundColor: 'rgba(59, 130, 246, 0.1),\n                            tension: 0.4,\n                            fill: true\n                        }]\n                    },\n                    options: {\n                        responsive: true,\n                        maintainAspectRatio: false,\n                        plugins: {\n                            legend: { position: 'top' }\n                        }\n                    }\n                }\n            );\n\n            // 费用分布图\n            this.expenseChart = new Chart(\n                document.getElementById('expenseChart').getContext('2d'), \n                {\n                    type: 'doughnut',\n                    data: {\n                        labels: ['人力成本', '营销费用', '研发投入', '行政开支', '其他费用'],\n                        datasets: [{\n                            data: [35, 25, 20, 15, 5],\n                            backgroundColor: [\n                                '#3B82F6', '#10B981', '#F59E0B', '#EF4444', '#8B5CF6'\n                            ]\n                        }]\n                    },\n                    options: {\n                        responsive: true,\n                        maintainAspectRatio: false\n                    }\n                }\n            );\n\n            // 预算执行情况图\n            this.budgetChart = new Chart(\n                document.getElementById('budgetChart').getContext('2d'), \n                {\n                    type: 'bar',\n                    data: {\n                        labels: ['收入', '利润', '成本', '现金流'],\n                        datasets: [{\n                            label: '完成率',\n                            data: [125, 114, 102, 135],\n                            backgroundColor: '#10B981'\n                        }]\n                    },\n                    options: {\n                        responsive: true,\n                        maintainAspectRatio: false,\n                        scales: {\n                            y: {\n                                beginAtZero: true,\n                                max: 150\n                        }\n                    }\n                }\n            );\n        }\n\n        updateCharts() {\n            const periodData = this.data[this.currentPeriod];\n            \n            // 更新图表数据\n            const labels = this.getLabelsForPeriod(this.currentPeriod);\n            \n            this.revenueChart.data.labels = labels;\n            this.revenueChart.data.datasets[0].data = periodData.revenue;\n            this.revenueChart.update();\n        }\n\n        getLabelsForPeriod(period) {\n            switch(period) {\n                case 'monthly':\n                    return ['1月', '2月', '3月', '4月', '5月', '6月', '7月', '8月', '9月', '10月', '11月', '12月'];\n                case 'quarterly':\n                    return ['Q1', 'Q2', 'Q3', 'Q4'];\n                case 'yearly':\n                    return ['2024年'];\n                default:\n                    return [];\n            }\n        }\n\n        refreshData() {\n            // 模拟数据刷新\n            this.showNotification('数据刷新完成');\n        }\n\n        showNotification(message) {\n            const notification = document.createElement('div');\n            notification.className = 'fixed top-4 right-4 bg-green-500 text-white px-6 py-3 rounded-lg shadow-lg fade-in';\n            notification.innerHTML = `\n                <div class=\"flex items-center\">\n                    <i class=\"fas fa-check-circle mr-2\"></i>\n                    ${message}\n                </div>\n            `;\n\n            document.body.appendChild(notification);\n\n            setTimeout(() => {\n                notification.remove();\n            }, 3000);\n        }\n    }\n\n    // 页面加载完成后初始化\n    document.addEventListener('DOMContentLoaded', function() {\n        const dashboard = new FinancialDashboard();\n    });\n</script>\n</body>\n</html>\n<code_end>\n<code_start project_name=财务统计看板 filename=dashboard_styles.css title=看板样式文件 entrypoint=false runnable=false project_final_file=false>\n/* 财务统计看板样式文件 */\n.dashboard-container {\n    max-width: 100%;\n    margin: 0 auto;\n}\n\n.stat-card {\n    background: white;\n    border-radius: 12px;\n    padding: 24px;\n    box-shadow: 0 4px 6px -1px rgba(0, 0, 0, 0.1);\n    transition: all 0.3s ease;\n    border-left: 4px solid #3b82f6;\n}\n\n.stat-card:hover {\n    transform: translateY(-2px);\n    box-shadow: 0 20px 25px -5px rgba(0, 0, 0, 0.1);\n}\n\n.chart-container {\n    background: white;\n    border-radius: 12px;\n    padding: 20px;\n    box-shadow: 0 4px 6px -1px rgba(0, 0, 0, 0.1);\n}\n\n.progress-bar {\n    height: 8px;\n    background: #e5e7eb;\n    border-radius: 4px;\n    overflow: hidden;\n}\n\n.progress-fill {\n    height: 100%;\n    background: linear-gradient(90deg, #10b981, #34d399);\n    transition: width 0.5s ease-in-out;\n}\n\n.status-badge {\n    padding: 4px 12px;\n    border-radius: 20px;\n    font-size: 12px;\n    font-weight: 500;\n}\n\n.status-completed {\n    background: #d1fae5;\n    color: #065f46;\n}\n\n.status-good {\n    background: #dbeafe;\n    color: #1e40af;\n}\n\n.status-warning {\n    background: #fef3c7;\n    color: #92400e;\n}\n\n.fade-in {\n    animation: fadeIn 0.5s ease-in;\n}\n\n@keyframes fadeIn {\n    from {\n        opacity: 0;\n        transform: translateY(10px);\n    }\n    to {\n        opacity: 1;\n        transform: translateY(0);\n    }\n}\n\n/* 响应式设计 */\n@media (max-width: 768px) {\n    .stat-card {\n        padding: 16px;\n    }\n    \n    .chart-container {\n        padding: 16px;\n    }\n}\n\n/* 表格样式优化 */\n.data-table {\n    width: 100%;\n    border-collapse: collapse;\n}\n\n.data-table th {\n    background: #f9fafb;\n    padding: 12px 16px;\n    text-align: left;\n    font-weight: 600;\n    color: #374151;\n    border-bottom: 2px solid #e5e7eb;\n}\n\n.data-table td {\n    padding: 12px 16px;\n    border-bottom: 1px solid #e5e7eb;\n}\n\n.data-table tr:hover {\n    background: #f9fafb;\n}\n<code_end>\n<code_start project_name=财务统计看板 filename=dashboard_functions.js title=看板功能脚本 entrypoint=false runnable=false project_final_file=false>\n// 财务看板功能函数库\nclass FinancialFunctions {\n    constructor() {\n        this.currencySymbol = '¥';\n        this.numberFormat = new Intl.NumberFormat('zh-CN');\n    }\n\n    // 格式化金额显示\n    formatCurrency(amount) {\n        return `${this.currencySymbol} ${this.numberFormat.format(amount)}`;\n    }\n\n    // 计算百分比变化\n    calculatePercentageChange(current, previous) {\n        if (previous === 0) return 100;\n        return ((current - previous) / previous) * 100;\n    }\n\n    // 格式化百分比显示\n    formatPercentage(value) {\n        return `${value > 0 ? '+' : ''}${value.toFixed(1)}%`;\n    }\n\n    // 生成随机数据（用于演示）\n    generateRandomData(baseValue, variance = 0.2) {\n        return baseValue * (1 + (Math.random() - 0.5) * variance);\n    }\n\n    // 数据验证\n    validateFinancialData(data) {\n        const errors = [];\n        \n        if (!data.revenue || data.revenue.length === 0) {\n            errors.push('收入数据不能为空');\n        }\n        \n        if (!data.expenses || data.expenses.length === 0) {\n            errors.push('支出数据不能为空');\n        }\n        \n        if (data.revenue && data.expenses) {\n            const totalRevenue = data.revenue.reduce((a, b) => a + b, 0);\n        const totalExpenses = data.expenses.reduce((a, b) => a + b, 0);\n        \n        if (totalExpenses > totalRevenue) {\n            errors.push('支出不能大于收入');\n        }\n        \n        return errors;\n    }\n\n    // 导出数据为CSV\n    exportToCSV(data, filename = 'financial_data') {\n        let csvContent = \"data:text/csv;charset=utf-8,\";\n        \n        // 添加表头\n        const headers = ['时期', '收入', '支出', '利润'];\n        csvContent += headers.join(',') + \"\\n\";\n        \n        // 添加数据行\n        data.forEach(row => {\n            csvContent += Object.values(row).join(',') + \"\\n\";\n        });\n        \n        const encodedUri = encodeURI(csvContent);\n        const link = document.createElement('a');\n        link.setAttribute('href', encodedUri);\n        link.setAttribute('download', `${filename}_${new Date().getTime()}.csv`);\n        document.body.appendChild(link);\n        link.click();\n        document.body.removeChild(link);\n    }\n}\n\n// 图表工具类\nclass ChartUtils {\n    static getChartColors() {\n        return {\n            primary: '#3b82f6',\n            secondary: '#10b981',\n            danger: '#ef4444',\n            warning: '#f59e0b',\n            info: '#6b7280',\n            success: '#22c55e'\n        };\n    }\n\n    static createGradient(ctx, color, opacity = 1) {\n        const gradient = ctx.createLinearGradient(0, 0, 0, 400);\n        gradient.addColorStop(0, `${color}${Math.floor(opacity * 255).toString(16).padStart(2, '0')}`;\n        return gradient;\n    }\n}\n\n// 事件处理器\nclass EventHandlers {\n    static handlePeriodChange(dashboard, event) {\n        dashboard.currentPeriod = event.target.value;\n        dashboard.updateCharts();\n    }\n\n    static handleRefresh(dashboard) {\n        dashboard.refreshData();\n    }\n\n    static handleExport() {\n        const functions = new FinancialFunctions();\n        functions.exportToCSV([\n            { period: '1月', revenue: 185, expense: 135, profit: 50 },\n            { period: '2月', revenue: 210, expense: 150, profit: 60 },\n            { period: '3月', revenue: 198, expense: 145, profit: 53 },\n            { period: '4月', revenue: 245, expense: 170, profit: 75 }\n        ]);\n    }\n}\n<code_end>\n<code_start project_name=财务统计看板 filename=requirements.txt title=项目依赖配置 entrypoint=false runnable=false project_final_file=true>\n# 财务统计看板项目依赖\n# 前端框架和库通过CDN引入，无需本地安装\n# 主要依赖：\n# - TailwindCSS (样式框架)\n# - Chart.js (数据可视化)\n# - Font Awesome (图标库)\n# 所有资源通过CDN加载，确保项目可独立运行\n<code_end>",
